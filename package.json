{
  "name": "biip-maps-web",
  "version": "1.0.0",
  "private": true,
  "prettier": "@aplinkosministerija/biip-prettier-config",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.vue,.ts",
    "lint:sarif": "set SARIF_ESLINT_EMBED=true && yarn run lint --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/vue": "^7.60.1",
    "@turf/buffer": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@vueform/multiselect": "^2.6.3",
    "@vueuse/core": "^10.9.0",
    "geojsonjs": "^0.1.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "ol": "^9.1.0",
    "ol-mapbox-style": "^12.3.0",
    "pinia": "^2.0.32",
    "proj4": "^2.9.0",
    "vue": "^3.2.47",
    "vue-markdown-render": "^2.1.1",
    "vue-router": "^4.1.6",
    "vue3-popper": "^1.5.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/vue3-recommended"
    ]
  },
  "devDependencies": {
    "@aplinkosministerija/biip-prettier-config": "^1.1.0",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "~4.8.4",
    "vite": "^4.5.3",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,vue}": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,html,css}": "prettier --write"
  }
}
